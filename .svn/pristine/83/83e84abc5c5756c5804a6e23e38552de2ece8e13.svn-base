html, body {
    height: 100%;
    margin: 0px;
    padding: 0px;
    min-width: 1200px;
    font-size: 14px;
}

@font-face {
    font-family: 'icomoon';
    src:  url('../../assets/font/icomoon.eot?k2gph');
    src:  url('../../assets/font/icomoon.eot?k2gph#iefix') format('embedded-opentype'),
    url('../../assets/font/icomoon.ttf?k2gph') format('truetype'),
    url('../../assets/font/icomoon.woff?k2gph') format('woff'),
    url('../../assets/font/icomoon.svg?k2gph#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-evaluate:before {
    content: "\e900";
}

.icon-download:before {
    content: "\e907";
}

.icon-back:before {
    content: "\e904";

}

.icon-video:before {
    content: "\e905";

}

.icon-help:before {
    content: "\e902";
}

.icon-group:before {
    content: "\e900";
    color: #bfbfbf;
}

.icon-account:before {
    content: "\e901";
}

.icon-question:before {
    content: "\e903";
    color: #595757;
}

.icon-refresh:before {
    content: "\ea2e";
}

body a {
    color: #20a0ff;
    text-decoration: none;
}

ul {
    list-style-type: none;
    padding: 0;
}

li {
    margin: 0 10px;
}

.margin-top {
    margin-top: 20px;
}

.margin-top-big {
    margin-top: 40px;
}

.margin-bottom {
    margin-bottom: 20px;
}

.float-right {
    float: right;
}

.view {
    margin: 20px 50px 50px 300px;
    overflow: hidden;
}

.main-container {
    margin: 0;
    overflow: hidden;
    height: 100%;
}

.el-breadcrumb {
    display: inline-block;
}

/*.add-button {
  font-size: 14px;
  position: relative;
  margin-bottom: 10px;
  color: #5ea0c2;
}*/

a:hover {
    color: #66b1ff;
}

.dropdown-link {
    cursor: pointer;
}

.el-row {
    margin-bottom: 15px;
}

.el-pagination {
    float: right;
    margin-top: 20px;
}

.el-pagination:after {
    clear: both;
}

.clear {
    clear: both;
}

.el-dialog__wrapper .el-dialog__header {
    padding: 20px 20px 10px;
}
.el-dialog__wrapper .el-dialog__body{
    padding: 30px;
}

.el-dialog__wrapper .el-dialog__footer{
    padding: 10px 20px 20px;
}

